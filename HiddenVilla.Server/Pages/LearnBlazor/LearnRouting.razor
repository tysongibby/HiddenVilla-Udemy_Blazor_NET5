@page "/learnrouting"
@page "/newrouting"
@page "/learnrouting/{parameter1}"
@page "/learnrouting/{parameter1}/{parameter2}"

<h3>Learn Routing</h3>
<h4>Parameter 1 : @Parameter1</h4>
<h4>Parameter 2 : @Parameter2</h4>

<NavLink class="btn btn-secondary" href="learnadvancedrouting?Param1=Blazing&Param2=Blazor">
    <span class="oi oi-list-rich" aria-hidden="true"></span> Routing with Query Params
</NavLink>

@code {
    [Parameter]
    public string Parameter1 { get; set; }
    [Parameter]
    public string Parameter2 { get; set; }
}
